includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true

    # Don't analyze config files since they're allowed to use env()
    # but we want to prevent env() calls in src/
    noEnvCallsOutsideOfConfig: true
